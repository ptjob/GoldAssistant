    <declare-styleable name="Emojicon">
        <attr name="emojiconSize" format="dimension" />
        <attr name="emojiconTextStart" format="integer" />
        <attr name="emojiconTextLength" format="integer" />
    </declare-styleable>
    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="show_mode" format="enum">
            <enum name="lay_down" value="0" />
            <enum name="pull_out" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FormItem">
        <attr name="topLine_shown" format="boolean"/>
        <attr name="bottomLine_shown" format="boolean"/>
        <attr name="form_icon" format="reference"/>
        <attr name="form_title" format="string"/>
        <attr name="form_value" format="string"/>
        <attr name="form_tips" format="string"/>
    </declare-styleable>

    <declare-styleable name="FormSetting">
        <attr name="set_topLine_shown" format="boolean"/>
        <attr name="set_bottomLine_shown" format="boolean"/>
        <attr name="form_left_txt" format="string"/>
        <attr name="form_Right_Img" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CountingEditText">
        <attr name="cet_countTextSize" format="dimension"/>
        <attr name="cet_countTextColor" format="color"/>
        <attr name="cet_maxLen" format="integer"/>
        <attr name="cet_countBarHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FormButton">
        <attr name="fb_topLine_shown" format="boolean"/>
        <attr name="fb_bottomLine_show" format="boolean"/>
        <attr name="fb_line_color" format="color"/>
        <attr name="fb_line_width" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CommonShowItemLayout">
        <attr name="csil_columnCount" format="integer"/>
        <attr name="csil_itemHeight" format="dimension"/>
        <attr name="csil_horizontalInnerMargin" format="dimension"/>
        <attr name="csil_verticleInnerMargin" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SelectLayout">
        <attr name="sl_textColor" format="color|reference"/>
        <attr name="sl_textSize" format="dimension"/>
        <attr name="sl_itemBackground" format="color|reference"/>
        <attr name="sl_maxSelectCount" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RankView">
        <attr name="rv_empty_img" format="reference"/>
        <attr name="rv_full_img" format="reference"/>
        <attr name="rv_total_score" format="integer"/>
        <attr name="rv_inner_margin" format="dimension"/>
    </declare-styleable>
